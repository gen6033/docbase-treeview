{
  "manifest_version": 3,
  "name": "DocBase Tree View",
  "description": "",
  "version": "1.0",
  "content_scripts": [
    {
      "js": ["content-script.js"],
      "css": ["content-style.css"],
      "matches": ["https://*.docbase.io/*"]
    }
  ],
  "permissions": ["webNavigation"],
  "web_accessible_resources": [{
    "resources": ["inline.js"],
    "matches": [
      "https://*.docbase.io/*"
    ]
  }]
}
